<Project Sdk="Microsoft.NET.Sdk">


	<PropertyGroup>
		<TargetFrameworks>net6.0;net7.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Version>1.6.1</Version>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Platforms>AnyCPU;x86</Platforms>
	</PropertyGroup>


	<ItemGroup>
	  <PackageReference Include="CSRedisCore" Version="3.8.671" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\AlbertCollection.Core\AlbertCollection.Core.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="SeedData\Json\deivce_plugin.json">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </None>
	  <None Update="SeedData\Json\device_collection.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	  <None Update="SeedData\Json\device_readdata.json">
